<?php //>

return new class() extends dungeons\web\backend\ListController {

    protected function init() {
        $table = table('{{ result.model }}');

        $names = [
{% for name in result.metadata|keys %}
            '{{ name }}',
{% endfor %}
        ];

        $this->table($table);
        $this->columns($table->getColumns($names));
    }

};
